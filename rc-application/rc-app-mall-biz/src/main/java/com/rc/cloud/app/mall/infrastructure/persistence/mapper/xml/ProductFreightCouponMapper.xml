<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qxun.qlive.goods.mapper.ProductFreightCouponMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.qxun.qlive.goods.model.ProductFreightCoupon">
        <result column="ID" property="id"></result>
        <result column="CreateTime" property="createTime"></result>
        <result column="ProductID" property="productID"></result>
        <result column="FullAmount" property="fullAmount"></result>
        <result column="AreaNameList" property="areaNameList"></result>
    </resultMap>

    <select id="getProductFreightCouponList" resultMap="BaseResultMap">
        SELECT [ID]
        , [CreateTime]
        , [ProductID]
        , [FullAmount]
        , [AreaNameList]
        FROM ProductFreightCoupon
        <where>
            
        </where>
    </select>
</mapper>